{
  "routes": [
    {
      "route": "/",
      "redirect": "/login.html"
    },
    {
      "route": "/login.html",
      "redirect": "/.auth/login/aad?post_login_redirect_uri=/page2.html"
    },
    {
      "route": "/.auth/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  }
}
